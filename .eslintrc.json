{
  "extends": "airbnb",
  "parserOptions": {
        "ecmaVersion": 2015
  },
  "env": {
      "browser": true,
      "node": true,
      "jasmine": true
  },
  "rules": {
      "quotes": "off",
      "semi": "off",
      "prefer-arrow-callback": "off",
      "func-names": "off",
      "prefer-const": ["error", {
          "destructuring": "all"
      }]
  }
}
